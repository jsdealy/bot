                case "f": rating  = 0
                case "d-": rating = 1
                case "d": rating  = 2
                case "d+": rating = 3
                case "c-": rating = 4
                case "c": rating  = 5
                case "c+": rating = 6
                case "b-": rating = 7
                case "b": rating  = 8
                case "b+": rating = 9
                case "a-": rating = 10
                case "a": rating  = 11
                case "a+": rating = 12
